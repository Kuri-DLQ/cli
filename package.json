{
  "name": "kuricli",
  "version": "0.0.0",
  "description": "oclif example Hello World CLI",
  "author": "",
  "bin": {
    "kuricli": "./bin/run"
  },
  "homepage": "https://github.com/anthonybeere/hello-world",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "anthonybeere/hello-world",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.131.0",
    "@oclif/core": "^1.9.9",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "@types/inquirer": "^8.2.1",
    "async": "^3.2.4",
    "axios": "^0.27.2",
    "child_process": "^1.0.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "fs": "^0.0.1-security",
    "inquirer": "^8.2.4",
    "listr": "^0.14.3",
    "ora": "^6.1.2",
    "process": "^0.11.10",
    "rsvp": "^4.8.5"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "kuricli",
    "dirname": "kuricli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "installAppDependencies": "cd ../app_server_express && npm install && cd ../cli",
    "installClientDepedencies": "cd ../dashboard && npm install && cd ../cli",
    "installCdkDependencies": "cd ../aws_infrastructure && npm install && cd ../cli",
    "bootstrapAWS": "cd ../aws_infrastructure && npm install && cdk bootstrap && cd ../cli",
    "deployMainInfrastructure": "cd ../aws_infrastructure && cdk deploy MainQueueAndDLQStack --require-approval never && ../cli",
    "deployDLQInfrastructure": "cd ../aws_infrastructure && cdk deploy DlqOnlyStack --require-approval never && ../cli"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/anthonybeere/hello-world/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
